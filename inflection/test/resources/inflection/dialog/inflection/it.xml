    <!--    isExists() checks-->
    <!--    Able to inflect to masculine using dictionary-->
    <test><source gender="masculine">benvenuto</source><result exists="true"/></test>
    <!--    Able to inflect to feminine using dictionary-->
    <test><source gender="feminine">benvenuto</source><result exists="true"/></test>
    <!--    Not able to inflect to feminine using dictionary-->
    <test><source gender="feminine">assideramento</source><result exists="false"/></test>
    <!--    Not able to inflect since word not in dictionary-->
    <test><source gender="feminine">qwyxor</source><result exists="false"/></test>
    <!--    Known and unknown word do not work-->
    <test><source number="plural">benvenuto qwyxor</source><result exists="false"/></test>
    <!--    Able to inflect unknown words to definite-->
    <test><source definiteness="definite">qwyxor</source><result exists="true"/></test>
    <!--    Unknown words definite inflection works but number inflect fails-->
    <test><source definiteness="definite" number="plural">qwyxor</source><result exists="false"/></test>

    <test><source gender="feminine" number="singular">benvenuto</source><result exists="true">benvenuta</result></test>
