<tokenizerTest locale="en">
    <!-- Patterns to trigger partial explicit token handling. -->
    <test>
        <annotatedTokens>^</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>^|key</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>^|key|^</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>^|key|^|=</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>^|key|^|=|^</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>^|key|^|=|^|value</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>^|key|^| |=|^|value|^</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>^|key|^|=| |^|value|^</annotatedTokens>
    </test>
    <test>
        <annotatedTokens>A|.|9</annotatedTokens>
    </test>
</tokenizerTest>
