    <!--    Able to inflect to masculine using dictionary-->
    <test><source gender="masculine">carro</source><result exists="true"/></test>
    <!--    Not able to inflect to feminine using dictionary-->
    <test><source gender="feminine">carro</source><result exists="false"/></test>
    <!--    Not able to inflect since word not in dictionary-->
    <test><source gender="feminine">qwyxor</source><result exists="false"/></test>
    <!--    Able to inflect-->
    <test><source gender="feminine">bem-vindo</source><result exists="true"/></test>
    <!--    Three word combination that didn't require dictionary to inflect-->
    <test><source number="plural">iPad Air 2</source><result exists="true"/></test>
    <!--    Known and unknown word do not work-->
    <test><source number="plural">vergalhão qwyxor</source><result exists="false"/></test>
    <!--    Not able to inflect verbs-->
    <test><source number="plural">chorar dormir</source><result exists="false"/></test>

